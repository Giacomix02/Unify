<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER" prefHeight="28.0" styleClass="footer" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
               <children>
                  <HBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                     <children>
                        <ImageView id="songImage" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </ImageView>
                        <Label id="songName" maxWidth="140.0" minWidth="27.0" text="Play a song">
                           <HBox.margin>
                              <Insets left="15.0" />
                           </HBox.margin>
               <padding>
                  <Insets right="15.0" />
               </padding>
               <font>
                  <Font size="16.0" />
               </font>
                        </Label>
                     </children>
                  </HBox>
                  <Slider id="sliderBar" prefHeight="14.0" prefWidth="573.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
                  </Slider>
                  <HBox alignment="CENTER_RIGHT" spacing="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <ImageView id="previousButton" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icons/backwards.png" />
                           </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                        </ImageView>
                        <ImageView id="playButton" fitHeight="25.0" fitWidth="25.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icons/play.png" />
                           </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                        </ImageView>
                        <ImageView id="nextButton" fitHeight="20.0" fitWidth="20.0" layoutX="50.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/icons/forwards.png" />
                           </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                        </ImageView>
                     </children>
                     <padding>
                        <Insets left="15.0" right="15.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="8.0" left="10.0" right="10.0" top="8.0" />
               </padding>
   <stylesheets>
      <URL value="@../app.css" />
      <URL value="@musicBar.css" />
   </stylesheets>
            </HBox>
